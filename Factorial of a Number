import java.util.Scanner;

class Solution {
	
	public static void main(String args[]) { 
		
		// Write code here
		int i,f=1;
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		if(n==0) 
		{
			System.out.println(1);
		}
		else if(n<0){
		System.out.println("Error");
		}
		else
		{
			for(i=n;i>0;i--)
			{
				f*=i;
			}
			System.out.println(f);
		}

	}
}
